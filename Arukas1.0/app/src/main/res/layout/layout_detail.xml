<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.xbw.arukas.DetailActivity"
    tools:showIn="@layout/activity_detail">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="20dp">

            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:text="Last update: "
                app:fontFamily="Muli-Regular.ttf"/>
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_update"
                app:fontFamily="Muli-Light.ttf"
                android:layout_centerVertical="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

        </RelativeLayout>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="@android:color/darker_gray"  />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/abc_background_cache_hint_selector_material_light">
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ID"
                android:layout_centerVertical="true"
                app:fontFamily="Muli-Regular.ttf"
                android:textSize="22sp"/>
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_id"
                app:fontFamily="Muli-Light.ttf"
                android:textSize="22sp"
                android:layout_centerVertical="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="@android:color/darker_gray"  />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/abc_background_cache_hint_selector_material_light">
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="App Name"
                android:layout_centerVertical="true"
                app:fontFamily="Muli-Regular.ttf"
                android:textSize="22sp"/>
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_appname"
                app:fontFamily="Muli-Light.ttf"
                android:textSize="22sp"
                android:layout_centerVertical="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="@android:color/darker_gray"  />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/abc_background_cache_hint_selector_material_light">
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Image	"
                android:layout_centerVertical="true"
                app:fontFamily="Muli-Regular.ttf"
                android:textSize="22sp"/>
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_image"
                app:fontFamily="Muli-Light.ttf"
                android:textSize="22sp"
                android:layout_centerVertical="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="@android:color/darker_gray"  />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/abc_background_cache_hint_selector_material_light">
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Instances"
                android:layout_centerVertical="true"
                app:fontFamily="Muli-Regular.ttf"
                android:textSize="22sp"/>
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_instances"
                app:fontFamily="Muli-Light.ttf"
                android:textSize="22sp"
                android:layout_centerVertical="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="@android:color/darker_gray"  />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/abc_background_cache_hint_selector_material_light">
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Memory"
                android:layout_centerVertical="true"
                app:fontFamily="Muli-Regular.ttf"
                android:textSize="22sp"/>
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_memory"
                app:fontFamily="Muli-Light.ttf"
                android:textSize="22sp"
                android:layout_centerVertical="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="@android:color/darker_gray"  />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/abc_background_cache_hint_selector_material_light">
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Endpoint"
                android:layout_centerVertical="true"
                app:fontFamily="Muli-Regular.ttf"
                android:textSize="22sp"/>
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_endpoint"
                app:fontFamily="Muli-Light.ttf"
                android:textSize="22sp"
                android:layout_centerVertical="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="@android:color/darker_gray"  />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/abc_background_cache_hint_selector_material_light">
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Port"
                android:id="@+id/t1"
                app:fontFamily="Muli-Regular.ttf"
                android:textSize="22sp"/>
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_port"
                app:fontFamily="Muli-Light.ttf"
                android:textSize="22sp"
                android:layout_below="@+id/t1" />
        </RelativeLayout>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="@android:color/darker_gray"  />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/abc_background_cache_hint_selector_material_light">
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ENV"
                android:layout_centerVertical="true"
                app:fontFamily="Muli-Regular.ttf"
                android:textSize="22sp"/>
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_env"
                app:fontFamily="Muli-Light.ttf"
                android:textSize="22sp"
                android:layout_centerVertical="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>
        <View
            android:layout_width="fill_parent"
            android:layout_height="1px"
            android:background="@android:color/darker_gray"  />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@color/abc_background_cache_hint_selector_material_light">
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="CMD"
                android:layout_centerVertical="true"
                app:fontFamily="Muli-Regular.ttf"
                android:textSize="22sp"/>
            <com.xbw.arukas.CustomTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_cmd"
                app:fontFamily="Muli-Light.ttf"
                android:textSize="22sp"
                android:layout_centerVertical="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />
        </RelativeLayout>

    </LinearLayout>
</android.support.v4.widget.NestedScrollView>